
int pinNumber=10;

void setup() {
  // initialize digital pin LED_BUILTIN as an output.
  pinMode(pinNumber, OUTPUT);
}

// the loop function runs over and over again forever
void loop() {
  
  digitalWrite(pinNumber, HIGH);   
  delay(200);                     
  digitalWrite(pinNumber, LOW);    
  delay(200);       
  digitalWrite(pinNumber, HIGH);   
  delay(200);      
  digitalWrite(pinNumber, LOW);    
  delay(200);  
  digitalWrite(pinNumber, HIGH);   
  delay(200);      
  digitalWrite(pinNumber, LOW);    
  delay(1400);       
  digitalWrite(pinNumber, HIGH);   
  delay(600);     
  digitalWrite(pinNumber, LOW);    
  delay(600);  
  digitalWrite(pinNumber, HIGH);   
  delay(600);     
  digitalWrite(pinNumber, LOW);    
  delay(600);  
  digitalWrite(pinNumber, HIGH);   
  delay(600);     
  digitalWrite(pinNumber, LOW);    
  delay(1400);  
  digitalWrite(pinNumber, HIGH);   
  delay(200);                     
  digitalWrite(pinNumber, LOW);    
  delay(200);       
  digitalWrite(pinNumber, HIGH);   
  delay(200);      
  digitalWrite(pinNumber, LOW);    
  delay(200);  
  digitalWrite(pinNumber, HIGH);   
  delay(200);      
  digitalWrite(pinNumber, LOW);    
  delay(1400);       
                
}
